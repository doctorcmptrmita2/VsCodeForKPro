{
	"name": "codexflow-agent",
	"displayName": "CodexFlow Agent",
	"description": "CodexFlow - AI-Powered Dev Team in Your Editor | LiteLLM Gateway | Decompose Pipeline | TL BazlÄ±",
	"publisher": "CodexFlow",
	"version": "1.0.7",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#1e40af",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0",
		"node": "20.19.2"
	},
	"author": {
		"name": "CodexFlow Team"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/codexflow/codexflow-agent"
	},
	"homepage": "https://codexflow.dev",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"codexflow",
		"codexflow-agent"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "codexflow-ActivityBar",
					"title": "CodexFlow Agent",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"codexflow-ActivityBar": [
				{
					"type": "webview",
					"id": "codexflow.SidebarProvider",
					"name": "CodexFlow Chat"
				}
			]
		},
		"commands": [
			{
				"command": "codexflow.plusButtonClicked",
				"title": "New Task",
				"icon": "$(edit)"
			},
			{
				"command": "codexflow.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "codexflow.marketplaceButtonClicked",
				"title": "%command.marketplace.title%",
				"icon": "$(extensions)"
			},
			{
				"command": "codexflow.popoutButtonClicked",
				"title": "%command.openInEditor.title%",
				"icon": "$(link-external)"
			},
			{
				"command": "codexflow.cloudButtonClicked",
				"title": "%command.cloud.title%",
				"icon": "$(cloud)"
			},
			{
				"command": "codexflow.settingsButtonClicked",
				"title": "%command.settings.title%",
				"icon": "$(settings-gear)"
			},
			{
				"command": "codexflow.openInNewTab",
				"title": "%command.openInNewTab.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.explainCode",
				"title": "%command.explainCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.fixCode",
				"title": "%command.fixCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.improveCode",
				"title": "%command.improveCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.addToContext",
				"title": "%command.addToContext.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.newTask",
				"title": "%command.newTask.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.terminalAddToContext",
				"title": "%command.terminal.addToContext.title%",
				"category": "Terminal"
			},
			{
				"command": "codexflow.terminalFixCommand",
				"title": "%command.terminal.fixCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "codexflow.terminalExplainCommand",
				"title": "%command.terminal.explainCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "codexflow.setCustomStoragePath",
				"title": "%command.setCustomStoragePath.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.importSettings",
				"title": "%command.importSettings.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.focusInput",
				"title": "%command.focusInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.acceptInput",
				"title": "%command.acceptInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "codexflow.toggleAutoApprove",
				"title": "%command.toggleAutoApprove.title%",
				"category": "%configuration.title%"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "codexflow.contextMenu",
					"group": "1"
				}
			],
			"codexflow.contextMenu": [
				{
					"command": "codexflow.addToContext",
					"group": "1_actions@1"
				},
				{
					"command": "codexflow.explainCode",
					"group": "1_actions@2"
				},
				{
					"command": "codexflow.improveCode",
					"group": "1_actions@3"
				}
			],
			"terminal/context": [
				{
					"submenu": "codexflow.terminalMenu",
					"group": "2"
				}
			],
			"codexflow.terminalMenu": [
				{
					"command": "codexflow.terminalAddToContext",
					"group": "1_actions@1"
				},
				{
					"command": "codexflow.terminalFixCommand",
					"group": "1_actions@2"
				},
				{
					"command": "codexflow.terminalExplainCommand",
					"group": "1_actions@3"
				}
			],
			"view/title": [
				{
					"command": "codexflow.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == codexflow.SidebarProvider"
				},
				{
					"command": "codexflow.settingsButtonClicked",
					"group": "navigation@2",
					"when": "view == codexflow.SidebarProvider"
				},
				{
					"command": "codexflow.cloudButtonClicked",
					"group": "navigation@3",
					"when": "view == codexflow.SidebarProvider"
				},
				{
					"command": "codexflow.marketplaceButtonClicked",
					"group": "navigation@4",
					"when": "view == codexflow.SidebarProvider"
				},
				{
					"command": "codexflow.historyButtonClicked",
					"group": "overflow@1",
					"when": "view == codexflow.SidebarProvider"
				},
				{
					"command": "codexflow.popoutButtonClicked",
					"group": "overflow@2",
					"when": "view == codexflow.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "codexflow.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == codexflow.TabPanelProvider"
				},
				{
					"command": "codexflow.settingsButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == codexflow.TabPanelProvider"
				},
				{
					"command": "codexflow.cloudButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == codexflow.TabPanelProvider"
				},
				{
					"command": "codexflow.marketplaceButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == codexflow.TabPanelProvider"
				},
				{
					"command": "codexflow.historyButtonClicked",
					"group": "overflow@1",
					"when": "activeWebviewPanelId == codexflow.TabPanelProvider"
				},
				{
					"command": "codexflow.popoutButtonClicked",
					"group": "overflow@2",
					"when": "activeWebviewPanelId == codexflow.TabPanelProvider"
				}
			]
		},
		"keybindings": [
			{
				"command": "codexflow.addToContext",
				"key": "cmd+k cmd+a",
				"mac": "cmd+k cmd+a",
				"win": "ctrl+k ctrl+a",
				"linux": "ctrl+k ctrl+a",
				"when": "editorTextFocus && editorHasSelection"
			},
			{
				"command": "codexflow.toggleAutoApprove",
				"key": "cmd+alt+a",
				"mac": "cmd+alt+a",
				"win": "ctrl+alt+a",
				"linux": "ctrl+alt+a"
			}
		],
		"submenus": [
			{
				"id": "codexflow.contextMenu",
				"label": "%views.contextMenu.label%"
			},
			{
				"id": "codexflow.terminalMenu",
				"label": "%views.terminalMenu.label%"
			}
		],
		"configuration": {
			"title": "%configuration.title%",
			"properties": {
				"codexflow.allowedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"git log",
						"git diff",
						"git show"
					],
					"description": "%commands.allowedCommands.description%"
				},
				"codexflow.deniedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"rm -rf /",
						"rm -rf ~",
						"rm -rf *",
						"Remove-Item -Recurse -Force /",
						"Remove-Item -Recurse -Force C:",
						"format",
						"mkfs",
						"dd if=",
						":(){:|:&};:",
						"chmod -R 777 /",
						"> /dev/sda",
						"npm install -g",
						"curl | bash",
						"wget | bash",
						"sudo rm"
					],
					"description": "%commands.deniedCommands.description%"
				},
				"codexflow.commandExecutionTimeout": {
					"type": "number",
					"default": 0,
					"minimum": 0,
					"maximum": 600,
					"description": "%commands.commandExecutionTimeout.description%"
				},
				"codexflow.commandTimeoutAllowlist": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%commands.commandTimeoutAllowlist.description%"
				},
				"codexflow.preventCompletionWithOpenTodos": {
					"type": "boolean",
					"default": false,
					"description": "%commands.preventCompletionWithOpenTodos.description%"
				},
				"codexflow.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.vendor.description%"
						},
						"family": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.family.description%"
						}
					},
					"description": "%settings.vsCodeLmModelSelector.description%"
				},
				"codexflow.customStoragePath": {
					"type": "string",
					"default": "",
					"description": "%settings.customStoragePath.description%"
				},
				"codexflow.enableCodeActions": {
					"type": "boolean",
					"default": true,
					"description": "%settings.enableCodeActions.description%"
				},
				"codexflow.autoImportSettingsPath": {
					"type": "string",
					"default": "",
					"description": "%settings.autoImportSettingsPath.description%"
				},
				"codexflow.maximumIndexedFilesForFileSearch": {
					"type": "number",
					"default": 10000,
					"minimum": 5000,
					"maximum": 500000,
					"description": "%settings.maximumIndexedFilesForFileSearch.description%"
				},
				"codexflow.useAgentRules": {
					"type": "boolean",
					"default": true,
					"description": "%settings.useAgentRules.description%"
				},
				"codexflow.apiRequestTimeout": {
					"type": "number",
					"default": 600,
					"minimum": 0,
					"maximum": 3600,
					"description": "%settings.apiRequestTimeout.description%"
				},
				"codexflow.newTaskRequireTodos": {
					"type": "boolean",
					"default": false,
					"description": "%settings.newTaskRequireTodos.description%"
				},
				"codexflow.codeIndex.embeddingBatchSize": {
					"type": "number",
					"default": 60,
					"minimum": 1,
					"maximum": 200,
					"description": "%settings.codeIndex.embeddingBatchSize.description%"
				},
				"codexflow.debug": {
					"type": "boolean",
					"default": false,
					"description": "%settings.debug.description%"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint . --ext=ts --max-warnings=0",
		"check-types": "tsc --noEmit",
		"pretest": "turbo run bundle --cwd ..",
		"test": "vitest run",
		"format": "prettier --write .",
		"bundle": "node esbuild.mjs",
		"vscode:prepublish": "pnpm bundle --production",
		"vsix": "mkdirp ../bin && vsce package --no-dependencies --out ../bin",
		"publish:marketplace": "vsce publish --no-dependencies && ovsx publish --no-dependencies",
		"watch:bundle": "pnpm bundle --watch",
		"watch:tsc": "cd .. && tsc --noEmit --watch --project src/tsconfig.json",
		"clean": "rimraf README.md CHANGELOG.md LICENSE dist logs mock .turbo"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.7.0",
		"@aws-sdk/client-bedrock-runtime": "^3.922.0",
		"@aws-sdk/credential-providers": "^3.922.0",
		"@google/genai": "^1.29.1",
		"@lmstudio/sdk": "^1.1.1",
		"@mistralai/mistralai": "^1.9.18",
		"@modelcontextprotocol/sdk": "1.12.0",
		"@qdrant/js-client-rest": "^1.14.0",
		"@roo-code/cloud": "workspace:^",
		"@roo-code/core": "workspace:^",
		"@roo-code/ipc": "workspace:^",
		"@roo-code/telemetry": "workspace:^",
		"@roo-code/types": "workspace:^",
		"@vscode/codicons": "^0.0.36",
		"async-mutex": "^0.5.0",
		"axios": "^1.12.0",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"diff-match-patch": "^1.0.5",
		"exceljs": "^4.4.0",
		"fast-deep-equal": "^3.1.3",
		"fast-xml-parser": "^5.0.0",
		"fastest-levenshtein": "^1.0.16",
		"fzf": "^0.5.2",
		"get-folder-size": "^5.0.0",
		"google-auth-library": "^9.15.1",
		"gray-matter": "^4.0.3",
		"i18next": "^25.0.0",
		"ignore": "^7.0.3",
		"isbinaryfile": "^5.0.2",
		"jwt-decode": "^4.0.0",
		"lodash.debounce": "^4.0.8",
		"mammoth": "^1.9.1",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
